@prefix dct: 		<http://purl.org/dc/terms/> .
@prefix dc: 		<http://purl.org/dc/elements/1.1/> .
@prefix void: 		<http://rdfs.org/ns/void#> .
@prefix dbp:		<http://dbpedia.org/property/> .

<#1> a void:Linkset ;
	dbp:script <scripts/ocd_get_and_sort.sh> ; 
	dbp:endpoint "http://dati.camera.it/sparql" ;
	dbp:constructquery "CONSTRUCT {?b <http://www.w3.org/2002/07/owl#sameAs> ?o} where { ?o owl:sameAs ?b. FILTER(REGEX(STR(?b),'http://dbpedia.org')) }" ;
	dbp:ntriplefilelocation <ocd_links.nt> ;
	dbp:outputFile <out1.nt> ;
	void:objectsTarget <https://datahub.io/dataset/dati-camera-it> ;
	dct:author <https://github.com/dvcama> ;
	dc:description "This is a complete catalogue of digital data and documents from the Legislature of the Kingdom of Sardinia to the fifteenth of the Republic, which supply the historical portal of the Chamber of Deputies. There are also the first datasets of Deputies, parliamentary groups and bodies of the current legislature." ;
